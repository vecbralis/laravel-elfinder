{
    "name": "vecbralis/laravel-elfinder",
    "keywords": ["laravel", "elfinder", "tinymce", "filebrowser"],
    "description": "A Laravel Package to integrate elFinder 2",
    "license": "MIT",
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        }
    ],
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "vecbralis/elfinder-flysystem-driver",
                "version":"master",
                "source": {
                    "url": "https://github.com/vecbralis/elfinder-flysystem-driver.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.x|5.1.x|5.2.x|5.3.x|5.4.x",
        "studio-42/elfinder": "~2.1.10",
        "vecbralis/elfinder-flysystem-driver": "master",
        "league/flysystem": "^1.0",
        "league/flysystem-cached-adapter": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Barryvdh\\Elfinder\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    },
    "minimum-stability": "dev"
}
